public class FizzBuzz{
for (byte i=1; i<=100; i++)
  {
    if (i % 3 == 0 || i % 5 == 0)
    {
      if (i % 3 != 0)
      {
        Console.WriteLine("Buzz");
      }
      else if (i % 5 != 0)
      {
        Console.WriteLine("Fizz");
      }
      else
      {
        Console.WriteLine("FizzBuzz");
      }
  }
  else
  {
    Console.WriteLine(i);
  }
}

}
